<script>
    const BeSellerApp = new Vue({
        el: '#BeSellerApp',
        data: {
            applyName: '',
            phone: '',
            shopName: '',
            account: '',
            email: '',
            sendFrom: '台灣',
            madeIn: '',
            socialMedia: '',
            introduction: '',
            idNumber: '',
            componient:'',
            componientWarn:'',
            idNumberWarn: '',
            checked: false,
            applyNameWarn: '',
            phoneWarn: '',
            shopNameWarn: '',
            accountWarn: '',
            emailWarn: '',
            madeInWarn: '',
            socialMediaWarn: '',
            introductionWarn: '',
            checkedWarn: '請選擇是否同意，感謝！',
            picWarn: '請提供 5 張照片',
            preview_list: [],
            image_list: [],
            warnCheck: '',
            isbusy: false,
        },
        watch: {
            componient: function () {
                let r = /^[\u4E00-\u9FFFA-Za-z]+$/
                if (!r.test(this.componient)) {
                    this.componientWarn = '「營業公司名稱」為必填項目，並且只能為英文或中文。'
                }
                else {
                    this.componientWarn = ''
                }
            },
            applyName: function () {
                let r = /^[\u4E00-\u9FFFA-Za-z]+$/
                if (!r.test(this.applyName)) {
                    this.applyNameWarn = '「申請人姓名」為必填項目，並且只能為英文或中文。'
                }
                else {
                    this.applyNameWarn = ''
                }
            },
            idNumber: function () {
                let r = /^[a-zA-Z][a-zA-Z0-9_]{4,15}$/
                if (!r.test(this.idNumber)) {
                    this.idNumberWarn = '「申請人身分證」為必填項目，並且只能為英文開頭，並且為英文或數字'
                }
                else {
                    this.idNumberWarn = ''
                }
            },
            phone: function () {
                let r = /^09[0-9]{8}$/
                if (!r.test(this.phone)) {
                    this.phoneWarn = '「手機號碼」為必填項目，並且為09開頭的10碼數字。'
                }
                else {
                    this.phoneWarn = ''
                }
            },
            shopName: function () {
                let r = /^[\u4E00-\u9FA5A-Za-z0-9_]+$/
                if (!r.test(this.shopName)) {
                    this.shopNameWarn = '「品牌名稱（設計館名稱）」為必填項目'
                }
                else {
                    this.shopNameWarn = ''
                }
            },
            account: function () {
                let r = /^[a-zA-Z][a-zA-Z0-9_]{4,15}$/
                if (!r.test(this.account)) {
                    this.accountWarn = '帳號只能使用英數字，並且使用英文開頭'
                }
                else {
                    this.accountWarn = ''
                }
            },
            email: function () {
                let r = /^\w+([-+.]\w+)*@@\w+([-.]\w+)*\.\w+([-.]\w+)*$/
                if (!r.test(this.email)) {
                    this.emailWarn = 'email為必填項目，並且格式要正確。'
                }
                else {
                    this.emailWarn = ''
                }
            },
            madeIn: function () {
                let r = /^[\u4E00-\u9FA5A-Za-z]+$/
                if (!r.test(this.madeIn)) {
                    this.madeInWarn = '「商品產地/體驗活動舉辦縣市」為必填項目，並且不能有數字。'
                }
                else {
                    this.madeInWarn = ''
                }
            },
            socialMedia: function () {
                let r = /(http|ftp|https):\/\/[\w\-_]+(\.[\w\-_]+)+([\w\-\.,@@?^=%&:/~\+#]*[\w\-\@@?^=%&/~\+#])?/
                if (!r.test(this.socialMedia)) {
                    this.socialMediaWarn = '請輸入 http(s):// 開頭的連結'
                }
                else {
                    this.socialMediaWarn = ''
                }
            },
            introduction: function () {
                if (this.introduction.trim().length <= 20) {
                    this.introductionWarn = '「品牌簡介」為必填項目，至少20字'
                }
                else {
                    this.introductionWarn = ''
                }
            },
            checked: function () {
                if (this.checked) {
                    this.checkedWarn = ''
                }
                else {
                    this.checkedWarn = '請選擇是否同意，感謝！'
                }
            }
        },
        computed: {
            dis() {
                if (this.applyName != '' && this.phone != '' && this.shopName != '' && this.account != '' && this.email != '' && this.madeIn != '' && this.socialMedia != '' && this.introduction != '' && this.idNumber != ''
                    && this.checked == true && this.applyNameWarn == '' && this.phoneWarn == '' && this.shopNameWarn == '' && this.accountWarn == '' && this.emailWarn == '' && this.idNumberWarn == ''
                    && this.madeInWarn == '' && this.socialMediaWarn == '' && this.introductionWarn == '' && this.picWarn == '') {
                    return false
                }
                else {
                    return true
                }
            }
        },
        methods: {
            previewMultiImage: function (event) {
                let input = event.target;
                let count = input.files.length;
                if (this.preview_list.length + count > 4) {
                    this.picWarn = '';
                }
                else {
                    this.picWarn = '請提供 5 張照片';
                }
                let index = 0;
                if (input.files) {
                    while (count--) {
                        let reader = new FileReader();
                        reader.onload = (e) => {
                            this.preview_list.push(e.target.result);
                        }
                        this.image_list.push(input.files[index]);
                        reader.readAsDataURL(input.files[index]);
                        index++;
                    }
                }
            },
            reset: function () {
                this.image_list = [];
                this.preview_list = [];
            },
            sendApply: function () {
                $('#AppltBtn').attr('disabled', true);
                let json = this
                json.isbusy = true
                let BeSellerDto = {
                    ApplyName: json.applyName,
                    Phone: json.phone,
                    ShopName: json.shopName,
                    Account: json.account,
                    Email: json.email,
                    SendFrom: json.sendFrom,
                    MadeIn: json.madeIn,
                    SocialMedia: json.socialMedia,
                    Introduction: json.introduction,
                    ImgList: json.preview_list,
                    IdNumber: json.idNumber,
                    Componient:json.componient
                }
                $.ajax({
                    url: '/api/User/ApplySeller',
                    type: 'POST',
                    contentType: "application/json",
                    data: JSON.stringify({
                        ApplyName: json.applyName,
                        Phone: json.phone,
                        Componient: json.shopName,
                        Account: json.account,
                        Email: json.email,
                        SendFrom: json.sendFrom,
                        MadeIn: json.madeIn,
                        SocialMedia: json.socialMedia,
                        Introduction: json.introduction,
                        ImgList: json.preview_list,
                        IdNumber: json.idNumber,
                        ShopName: json.componient
                    }),
                    success: function (output) {
                        json.isbusy = false
                        if (output.result.warn == "") {
                            json.warnCheck = ""
                            window.location.href = "/User/Finish"
                        }
                        else {
                            json.warnCheck = output.result.warn
                        }
                        $('#AppltBtn').attr('disabled', false);
                    }
                });
            }
        }
    })
</script>
