            <template #overlay>
                <div class="text-center text-danger my-2">
                    <b-spinner class="align-middle">
                    </b-spinner>
                    <strong>Loading...</strong>
                </div>
            </template>
            <b-input-group size="lg" v-bind:aria-hidden="!isbusy">
                <b-button id="AppltBtn" class="btn bg-main mx-auto text-white" v-on:click="sendApply" v-bind:disabled="dis">送出</b-button>
            </b-input-group>
