@{
    ViewData["Title"] = "Home Page";
    Layout = "~/Views/Shared/_Layout_FrontEnd.cshtml";
}
<div class="pt-146 container py-4">
    <h2 class="text-main my-5 fw-bolder fs-4 text-center">請先登入你的 Hatsukoi 帳號！</h2>
    <div class="max-500 mx-auto">
        <img src="~/img/user/submitted-successfully.png" />
    </div>
    <div class="max-500 border rounded mx-auto text-small p-4">
        <p class="my-3">再次謝謝你的耐心等待，歡迎來到 Hatsukoi！</p>
        <button id="open" class="btn my-4 my-btn d-block mx-auto p-2 px-3">登入</button>
    </div>
</div>
<div id="test"></div>
<script src="~/lib/jquery/dist/jquery.min.js"></script>
@section topCSS{
    <link href="~/css/user/finishseller.css" rel="stylesheet" />
}
<script> 
    $('#open').click(function () {
        $('#test').load("@Url.Action("OpenLoginModal","Login")")
    })
    $('#openSecond').click(function () {
        $('#test').load("@Url.Action("OpenLoginModal","Login")")
    })
</script>